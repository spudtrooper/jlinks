{
  "manifest_version": 3,
  "name": "J Links",
  "version": "1.0",
  "description": "Like go links but with a J",
  "permissions": [
    "storage",
    "chrome.tabs",
    "activeTab"
  ],
  "module": true,
  "omnibox": {
    "keyword": "j"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.mjs",
    "type": "module"
  }
}